    "params_decision_tree = {\n",
    "    'criterion': 'gini',  # or 'entropy'\n",
    "    'max_depth': 5,\n",
    "    'min_samples_split': 2,\n",
    "    'random_state': 42\n",
    "}\n",
    "\n",
    "estimators = [\n",
    "    ('dt', DecisionTreeClassifier(**params_decision_tree)),\n",
    "    ('xgb', XGBClassifier(**xgb_params)),\n",
    "    ('cat', CatBoostClassifier(**params_catboost))\n",
    "]\n",
    "log_meta = LogisticRegression(\n",
    "    solver='liblinear',\n",
    "    random_state=42,\n",
    "    class_weight='balanced',\n",
    "    max_iter=1000\n",
    ")\n",
    "\n",
    "stacking_clf = StackingClassifier(\n",
    "    estimators=estimators,\n",
    "    final_estimator=log_meta,\n",
    "    passthrough=True,\n",
    "    cv=5\n",
    ")\n",
    "\n"
